{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AxiomRdlCore": {
    "Parsing": {
      "MaxPageCount": 100,
      "OcrEnabled": true,
      "OcrLanguages": ["en", "zh-Hans"],
      "OcrConfidenceThreshold": 0.8
    },
    "Generation": {
      "DefaultPageWidth": "8.5in",
      "DefaultPageHeight": "11in",
      "DefaultMargins": {
        "Top": "0.5in",
        "Bottom": "0.5in",
        "Left": "0.75in",
        "Right": "0.75in"
      },
      "EmbedImages": true,
      "MaxImageResolution": 300
    },
    "Validation": {
      "StrictSchemaValidation": true,
      "ExpressionSandboxMode": true,
      "VisualComparisonThreshold": 0.95
    }
  }
}
